{
  "manifest_version": 3,
  "name": "translator",
  "version": "1.1",
  "description": "Translates a selected text using Lingua Leo API",
  "icons": {
    "64": "dictionary.png"
  },
  "content_scripts": [
    {
      "js": ["translator.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["fp.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
